@page "/Languages/HighVersianLangauge/AllTerms"
@using WikiServices.DataServices
@inject HighVersianService highVersian
<head>
    <title>High Versian</title>
</head>
<style>
    .high {
        font-family: 'high_versianregular';
    }
</style>

<body>
<h1>High Versian All Terms</h1>
<a href="/Languages/HighVersianLangauge/">Back to language page.</a>
<p>The complete list of terms in High Versian that are in the dictionary.</p>
    <ul>
@foreach (var term in highVersian.GetAllTerms().OrderBy(x => x.Key.Letters))
{
            <li><b class="high">@term.Key.Letters</b> (@term.Value) : @term.Key.Meaning</li>
}
    </ul>
</body>